{
  "name": "image-cutout-app",
  "version": "1.0.1",
  "private": true,
  "description": "该应用用于图片切块。",
  "main": "electron/main.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "dev": "concurrently \"npm run serve\" \"wait-on http://localhost:8080 && cross-env NODE_ENV=development electron ./electron/main.js\"",
    "dist": "npm run build && electron-builder --publish=always"
  },
  "build": {
    "appId": "com.example.myvueapp",
    "productName": "图片切块应用测试版",
    "directories": {
      "output": "release"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "yabaizzz",
        "repo": "ImageCutoutApp",
        "releaseType": "release",
        "private": true
      }
    ],
    "files": [
      "dist/**/*",
      "electron/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "target": "dmg"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "dependencies": {
    "@dataview/datav-vue3": "^0.0.0-test.1672506674342",
    "autofit.js": "^3.2.2",
    "axios": "^1.12.2",
    "core-js": "^3.8.3",
    "echarts": "^5.5.1",
    "echarts-liquidfill": "^3.1.0",
    "electron-updater": "^6.6.2",
    "element-plus": "^2.11.3",
    "moment": "^2.30.1",
    "pinia": "^3.0.3",
    "pixi.js": "^8.14.0",
    "qs": "^6.14.0",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "vue-tianditu": "^2.7.6",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "vue-cli-plugin-electron-builder": "^2.1.1",
    "wait-on": "^9.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
